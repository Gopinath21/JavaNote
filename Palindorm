class Palindom{
public static void main(String  []){
  String s="aabbaa";
  count=0;
      for(int i=0;i<s.lenght();i++)
      {
          if(s.charAt(i)==s.charAt(s.length()-1-i){}
          else{
          count++;}
        
      }
      if(count==0){
        System.out.println("palindrom");
      }
      else{
      System.out.println("not a palidrom");
      }
  
}

}
